{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-curl": "^7.2",
        "dawood/phpscreenrecorder": "^1.4",
        "embed/embed": "^3.4",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.5",
        "highideas/laravel-users-online": "^3.0",
        "intervention/image": "^2.5",
        "jenssegers/agent": "^2.6",
        "laracasts/utilities": "^3.0",
        "laravel-notification-channels/pusher-push-notifications": "^1.1",
        "laravel/framework": "^6.0",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/socialite": "^4.3",
        "laravel/tinker": "^1.0",
        "laravolt/avatar": "^3.2",
        "maatwebsite/excel": "^3.1",
        "nesbot/carbon": "^2.25",
        "phpdocumentor/graphviz": "^1.0",
        "pusher/pusher-php-server": "^4.1",
        "realrashid/sweet-alert": "^3.1",
        "renatomarinho/laravel-page-speed": "^1.8",
        "scrivo/highlight.php": "^9.15",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "spatie/db-dumper": "^2.15",
        "yajra/laravel-datatables-fractal": "^1.5",
        "yajra/laravel-datatables-oracle": "^9.10"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fruitcake/laravel-telescope-toolbar": "^1.2",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^6.2",
        "laravel/telescope": "^2.1",
        "laravel/ui": "^1.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpro/grumphp": "dev-master",
        "phpunit/phpunit": "^7.5"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/Translator.php",
            "app/Helpers/DateHelper.php",
            "app/Helpers/Internet.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }

}
